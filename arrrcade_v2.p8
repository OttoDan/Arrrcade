--game loop
function _init()
 objects.blocks:create(64,32,obj_chest)
 objects.chars:create(32,48,obj_cade)

end

function _update()

end

function _draw()
 cls(4)
 palt(0,false)--black -> opaque
 palt(11,true)--green -> transparent
 objects:draw()
 --debug print
 print("cpu: "..stat(1)
  .."\nblc: "..#objects.blocks
  .."\nchr: "..#objects.chars
  .."\nitm: "..#objects.items
  .."\npjt: "..#objects.projectiles
 )
end

